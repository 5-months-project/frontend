<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="beige">#B4A79E</color>
    <color name="black">#FF000000</color>
    <color name="brown">#BDA18C</color>
    <color name="dark_brown">#3F352C</color>
    <color name="gray">#757575</color>
    <color name="group_create">#D9DED8</color>
    <color name="light_beige">#F1ECE1</color>
    <color name="purple_200">#FFBB86FC</color>
    <color name="purple_500">#FF6200EE</color>
    <color name="purple_700">#FF3700B3</color>
    <color name="sidemenu_background">#F1ECE1</color>
    <color name="teal_200">#FF03DAC5</color>
    <color name="teal_700">#FF018786</color>
    <color name="white">#FFFFFFFF</color>
    <string name="app_name">frontend</string>
    <style name="Theme.Frontend" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:forceDarkAllowed">false</item>
        
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        
        <item name="android:statusBarColor">@color/white</item>
        
    </style>
    <style name="calendarcell" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">55dp</item>
        <item name="android:layout_height">70dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@color/light_beige</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="largecal" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="fontFamily">@font/font_lineseedkr_rg</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="progressbar" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center</item>
        <item name="fontFamily">@font/font_lineseedkr_rg</item>
        <item name="android:textStyle">bold</item>


    </style>
    <style name="smallcal" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center</item>
        <item name="fontFamily">@font/font_lineseedkr_rg</item>
        <item name="android:textStyle">bold</item>
    </style>
</resources>